{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "import/prefer-default-export": 0,
        "import/no-extraneous-dependencies": 0,
        "no-underscore-dangle": 0,
        "no-console": 0,
        "no-undef": 0,
        "no-unused-vars": 0, 
        "no-alert": 0
    }
}
